<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activity.AerobicoActivity"
    tools:showIn="@layout/activity_aerobico">


    <androidx.viewpager.widget.ViewPager
        android:id="@+id/vp_pagina_aerobico_cadastro"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stl_tabs_aerobico_cadastro"></androidx.viewpager.widget.ViewPager>

    <com.campos.william.academiatcc.helper.SlidingTabLayout
        android:id="@+id/stl_tabs_aerobico_cadastro"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toTopOf="@+id/vp_pagina_aerobico_cadastro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></com.campos.william.academiatcc.helper.SlidingTabLayout>







</androidx.constraintlayout.widget.ConstraintLayout>